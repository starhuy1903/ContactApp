<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.image.ImageView?>
<AnchorPane fx:id="ap_main" xmlns:fx="http://javafx.com/fxml"
            prefWidth="600" prefHeight="400"
            fx:controller="com.server.Controller">
    <children>
        <Label layoutX="150" layoutY="26" text="Contact Server">
            <font>
                <Font name="Arial Rounded MT Bold" size="23"/>
            </font>
        </Label>
        <ScrollPane fx:id="sp_main" layoutX="30" layoutY="70"
                prefWidth="540" prefHeight="260" fitToWidth="true">
            <content>
                <VBox fx:id="vbox_messages" prefWidth="534" prefHeight="252"/>
            </content>
        </ScrollPane>
        <Button onAction="#startServer" text="Start Server"/>
    </children>

</AnchorPane>

